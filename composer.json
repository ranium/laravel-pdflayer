{
    "name": "mathieutu/laravel-pdflayer",
    "description": "A pdflayer API bridge for Laravel",
    "type": "package",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/laravel": "^8.0",
        "mockery/mockery": "^1.3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu TUDISCO",
            "email": "dev@mathieutu.ovh"
        }
    ],
    "autoload": {
        "psr-4": {
            "MathieuTu\\PDFLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MathieuTu\\PDFLayer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MathieuTu\\PDFLayer\\PDFLayerServiceProvider"
            ],
            "aliases": {
                "PDF": "MathieuTu\\PDFLayer\\Facades\\PDF"
            }
        }
    },
    "minimum-stability": "stable"
}
